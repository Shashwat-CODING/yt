client
dev tun
proto udp
remote your-indian-vpn-server.example.com 1194
resolv-retry infinite
nobind
persist-key
persist-tun
remote-cert-tls server
verify-x509-name server_VKQN52Djft55SSZC name
auth SHA256
auth-nocache
cipher AES-128-GCM
tls-client
tls-version-min 1.2
tls-cipher TLS-ECDHE-RSA-WITH-AES-128-GCM-SHA256
ignore-unknown-option block-outside-dns
setenv opt block-outside-dns
verb 3

# Add your certificates and keys here
# Alternatively, you can load them from environment variables using auth-user-pass
# or from separate files

# Authentication
auth-user-pass

# CA Certificate
<ca>
# Paste your CA certificate here
</ca>

# Client Certificate
<cert>
# Paste your client certificate here
</cert>

# Client Key
<key>
# Paste your client key here
</key>

# TLS Auth Key
<tls-auth>
# Paste your TLS auth key here
</tls-auth>

# The following section enables using environment variables from Render
# The script will automatically replace these placeholders with actual credentials
# that you set in your Render environment variables
<auth-user-pass>
##VPN_USERNAME##
##VPN_PASSWORD##
</auth-user-pass>